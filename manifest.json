{
  "manifest_version": 2,
  "name": "cleanWeb-广告杀手",
  "description": "网页广告杀手",
  "version": "1.0",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "webRequestBlocking",
    "webRequest"
  ],
  "icons": {
    "16": "icons/128.png",
    "48": "icons/128.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/128-gray.png",
      "48": "icons/128-gray.png",
      "128": "icons/128-gray.png"
    },
    "default_popup": "popups/popup.html"
  },
  "background": {
    "scripts": [
      "./script/background.js"
    ]
  }
}